{
  "name": "stock-analysis-webapp",
  "version": "1.0.0",
  "description": "High-performance QQQ/TQQQ drawdown analysis web application",
  "main": "backend/server.js",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "nodemon backend/server.js",
    "frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "node backend/server.js",
    "install-all": "npm install && cd frontend && npm install"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "sqlite3": "^5.1.6",
    "pandas": "^2.1.4",
    "python-shell": "^5.0.0",
    "multer": "^1.4.5-lts.1",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "keywords": ["stock", "analysis", "qqq", "tqqq", "drawdown", "react", "express"],
  "author": "Stock Analysis Team",
  "license": "MIT"
}
